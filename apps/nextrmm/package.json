{
  "name": "@nextrmm/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate:reset": "prisma migrate reset --force",
    "dev": "next dev",
    "format": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --pretty --noEmit",
    "start": "next start",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next"
  },
  "dependencies": {
    "@nextrmm/ui": "workspace:*",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.4.2",
    "@t3-oss/env-nextjs": "^0.7.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "react": "18.2.0",
    "react-hook-form": "^7.47.0",
    "lucide-react": "^0.288.0",
    "next": "^13.5.4",
    "next-auth": "^4.23.0",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.7",
    "resend": "^1.1.0",
    "superjson": "^1.13.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nextrmm/tsconfig": "workspace:*",
    "@nextrmm/eslint-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "prisma": "^5.4.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "prettier": "^3.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.5.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.22.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "prisma/schema.prisma": [
      "prisma format"
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
