{
  "extends": "@nextrmm/tsconfig/next.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
    "plugins": [{ "name": "next" }]
  },
  "include": [
    ".eslintrc.json",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts",
    "types/*.d.ts"
  ],
  "exclude": ["node_modules"]
}
